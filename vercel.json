{
  "rewrites": [
    {
      "source": "/img/(.*)",
      "destination": "/api/image?fileId=$1"
    },
    {
      "source": "/s/(.*)",
      "destination": "/api/proxy?path=$1"
    },
    {
      "source": "/health",
      "destination": "/api/health"
    },
    {
      "source": "/",
      "destination": "/api/health"
    }
  ]
}
